{
  "name": "svelte",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
